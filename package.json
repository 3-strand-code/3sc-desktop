{
  "name": "3sc-desktop",
  "version": "1.0.0",
  "description": "3 Strand Code's desktop app",
  "productName": "3StrandCode",
  "main": "main.js",
  "scripts": {
    "build": "cross-env NODE_ENV=production webpack --config webpack.config.production.js --progress --profile --colors",
    "hot:server": "DEBUG=3sc* node server.js",
    "lint": "eslint .",
    "lint:fix": "npm run lint -- --fix",
    "package": "cross-env NODE_ENV=production babel-node package.js",
    "package:all": "npm run package -- --all",
    "postinstall": "node node_modules/fbjs-scripts/node/check-dev-engines.js package.json",
    "start": "cross-env NODE_ENV=production electron ./",
    "start:hot": "cross-env HOT=1 DEBUG=3sc* NODE_ENV=development electron ./",
    "test": "cross-env NODE_ENV=test mocha --compilers js:babel-core/register --recursive --require ./test/setup.js test/**/*.spec.js",
    "test:e2e": "cross-env NODE_ENV=test mocha --compilers js:babel-core/register --require ./test/setup.js --require co-mocha ./test/e2e.js",
    "test:watch": "npm test -- --watch"
  },
  "bin": {
    "electron": "./node_modules/.bin/electron"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/3-strand-code/3sc-desktop.git"
  },
  "author": {
    "name": "Levi Thomason",
    "email": "levi@3strandcode.com",
    "url": "https://github.com/3-strand-code"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/3-strand-code/3sc-desktop/issues"
  },
  "keywords": [
    "3SC",
    "3 Strand Code",
    "Desktop",
    "windows",
    "mac",
    "linux"
  ],
  "homepage": "https://github.com/3-strand-code/3sc-desktop#readme",
  "devDependencies": {
    "asar": "^0.9.0",
    "babel-core": "^6.3.26",
    "babel-eslint": "^5.0.0",
    "babel-loader": "^6.2.0",
    "babel-plugin-add-module-exports": "^0.1.2",
    "babel-plugin-webpack-loaders": "^0.2.1",
    "babel-polyfill": "^6.3.14",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-react-hmre": "^1.0.1",
    "babel-preset-stage-0": "^6.3.13",
    "chai": "^3.3.0",
    "chromedriver": "^2.19.0",
    "co-mocha": "^1.1.2",
    "cross-env": "^1.0.7",
    "css-loader": "^0.23.1",
    "debug": "^2.2.0",
    "del": "^2.0.1",
    "dirty-chai": "^1.2.2",
    "electron-packager": "^5.2.0",
    "electron-prebuilt": "^0.36.2",
    "electron-rebuild": "^1.0.0",
    "eslint": "^1.3.1",
    "eslint-config-ta": "^4.1.0",
    "eslint-plugin-react": "^3.13.1",
    "express": "^4.13.3",
    "extract-text-webpack-plugin": "^0.9.1",
    "fbjs-scripts": "^0.5.0",
    "jsdom": "^7.2.2",
    "json-loader": "^0.5.4",
    "minimist": "^1.2.0",
    "mocha": "^2.3.0",
    "node-libs-browser": ">= 0.4.0 <=0.6.0",
    "react-addons-test-utils": "^0.14.2",
    "redux-devtools": "^3.0.1",
    "redux-devtools-dock-monitor": "^1.0.1",
    "redux-devtools-log-monitor": "^1.0.1",
    "redux-logger": "^2.3.1",
    "selenium-webdriver": "^2.48.2",
    "sinon": "^1.17.2",
    "style-loader": "^0.13.0",
    "webpack": "^1.12.1",
    "webpack-dev-middleware": "^1.2.0",
    "webpack-hot-middleware": "^2.4.1",
    "webpack-target-electron-renderer": "^0.3.0"
  },
  "dependencies": {
    "electron-debug": "^0.5.1",
    "font-awesome": "^4.4.0",
    "react": "^0.14.2",
    "react-dom": "^0.14.2",
    "react-redux": "^4.0.5",
    "react-router": "^2.0.0",
    "react-router-redux": "^3.0.0",
    "redux": "^3.2.1",
    "redux-promise": "^0.5.0",
    "redux-thunk": "^1.0.2"
  },
  "devEngines": {
    "node": "4.x || 5.x",
    "npm": "2.x || 3.x"
  }
}
